{
    "extends": ["next/core-web-vitals", "prettier"],
    "plugins": ["prettier"],
    "globals": {
        "React": "readonly"
    },
    "rules": {
        "prettier/prettier": "error",
        "no-console": "warn",
        "no-unused-vars": "warn",
        "react-hooks/rules-of-hooks": "warn",
        "react-hooks/exhaustive-deps": 0
    }
}
